{
    "name": "react-form-enhancer",
    "version": "0.1.5",
    "description": "This library enhances React form components with React's basic state & property.",
    "main": "./lib/index.js",
    "types": "./lib/index.d.ts",
    "author": "masataka-kurihara",
    "license": "MIT",
    "homepage": "https://github.com/masataka-kurihara/react-form-enhancer",
    "bugs": {
        "url": "https://github.com/masataka-kurihara/react-form-enhancer/issues"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/masataka-kurihara/react-form-enhancer"
    },
    "keywords": [
        "react",
        "form",
        "validation",
        "submit",
        "redux",
        "typescript",
        "javascript"
    ],
    "scripts": {
        "lint": "tslint --fix --project ./ --type-check './src/**/*.{ts,tsx}'",
        "test": "jest",
        "build": "rimraf ./lib/* && tsc",
        "doc": "rimraf ./doc/* && typedoc --out ./doc ./src/index.ts"
    },
    "jest": {
        "moduleFileExtensions": [
            "ts",
            "tsx",
            "js"
        ],
        "setupFiles": [
            "./tools/enzyme.setup.js"
        ],
        "transform": {
            "^.+\\.(ts|tsx)$": "./node_modules/ts-jest/preprocessor.js"
        },
        "testMatch": [
            "**/__tests__/*((Test|Spec)\\.(ts|tsx))"
        ]
    },
    "devDependencies": {
        "@types/enzyme": "^2.8.7",
        "@types/jest": "^20.0.8",
        "@types/prop-types": "^15.5.1",
        "@types/react": "^16.0.5",
        "enzyme": "^2.9.1",
        "jest": "^21.1.0",
        "react-dom": "^15.6.1",
        "react-test-renderer": "^15.6.1",
        "rimraf": "^2.6.2",
        "ts-jest": "^21.0.1",
        "tslint": "^5.7.0",
        "tslint-config-airbnb": "^5.3.0",
        "tslint-react": "^3.2.0",
        "typedoc": "^0.8.0",
        "typescript": "^2.5.2"
    },
    "dependencies": {
        "immutable": "^3.8.1",
        "prop-types": "^15.5.10",
        "react": "^15.6.1"
    }
}
